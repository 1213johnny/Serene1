@model List<Serene1.Default.EmployeeRow>

@if ((bool?)ViewData["Printing"] == true)
{
    Layout = MVC.Views.Shared._LayoutNoNavigation;
}
<body>
    <table
        align="center"
        style="font-family:標楷體;">             
        <tr bgcolor = gray>
            <th >員工編號</th>
            <th>中文姓名</th>
            <th>英文姓名</th>
            <th>識別證號碼</th>
            <th>單位</th>
            <th>部門別</th>
        </tr>
        @foreach(var data in Model){
            <tr>
                <th>@data.EmployeeId</th>
                <th>@data.UnitId</th>
                <th>@data.EmployeeName</th>
                <th>@data.IdentificationCardNo</th>
                <th>@data.UnitId</th>
                <th>@data.DeptType</th>
            </tr>
        }
    </table>
</body>